{
	"name": "iconick/gallery-quest",
	"description": "A filterable gallery block plugin for WordPress",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Iconick",
			"homepage": "https://iconick.io"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"wp-coding-standards/wpcs": "^3.0",
		"phpstan/phpstan": "^1.10",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0"
	},
	"scripts": {
		"phpcs": "phpcs",
		"phpcbf": "phpcbf",
		"phpstan": "phpstan analyse",
		"test": "phpunit"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}

