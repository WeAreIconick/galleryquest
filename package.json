{
	"name": "gallery-quest",
	"version": "1.0.2",
	"description": "A filterable gallery block plugin for WordPress",
	"author": "Iconick",
	"homepage": "https://iconick.io",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"build:dist": "node scripts/build-dist.js",
		"build:auto": "node scripts/auto-build.js",
		"dev": "node scripts/watch.js",
		"watch": "wp-scripts start",
		"format": "wp-scripts format",
		"format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"lint": "npm run lint:js && npm run lint:css && npm run lint:php",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "eslint . --ext .js --max-warnings 0",
		"lint:js:fix": "eslint . --ext .js --fix",
		"lint:php": "phpcs",
		"lint:php:fix": "phpcbf",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"test": "jest --watch",
		"test:ci": "jest --ci --coverage --maxWorkers=2",
		"test:coverage": "jest --coverage",
		"test:php": "phpunit",
		"test:php:coverage": "phpunit --coverage-html coverage/php",
		"analyze:php": "phpstan analyse",
		"check": "npm run lint && npm run test:ci",
		"check:all": "npm run lint && npm run test:ci && npm run analyze:php",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "node scripts/watch.js",
		"postinstall": "npm run build",
		"prepare": "husky install || true"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.28.5",
		"@babel/preset-react": "^7.28.5",
		"@wordpress/scripts": "^31.0.0",
		"babel-jest": "^30.2.0",
		"eslint": "^8.57.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^24.1.3",
		"prettier": "^3.6.2"
	},
	"dependencies": {
		"photoswipe": "^5.4.4"
	}
}
